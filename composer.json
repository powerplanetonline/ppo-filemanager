{
  "name": "powerplanetonline/ppo-filemanager",
  "description": "A file upload/editor intended for use with Laravel 5 and CKEditor / TinyMCE",
  "license": "MIT",
  "keywords": [
    "filemanager",
    "laravel",
    "ckeditor",
    "tinymce",
    "upload",
    "file",
    "manager",
    "image"
  ],
  "authors": [
    {
      "name": "Alberto Iniesta Gil",
      "email": "alberto.iniesta@powerplanetonline.com"
    },
    {
      "name": "PowerPlanetOnline",
      "email": "it@powerplanetonline.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": ">=4.2.0",
    "intervention/image": "2.*"
  },
  "suggest": {
    "ext-gd": "to use GD library based image processing.",
    "ext-imagick": "to use Imagick based image processing."
  },
  "autoload": {
    "psr-4": {
      "Powerplanetonline\\PpoFilemanager\\": "src/"
    }
  }
}
